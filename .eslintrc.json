{
  "env": {
      "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": false
      }
    ],
    "global-require": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": [
        "tests.js"
      ]
    }],
    "object-curly-newline": 0
  }
}
